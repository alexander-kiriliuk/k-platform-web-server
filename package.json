{
  "name": "@k-platform/web-server",
  "version": "0.0.1",
  "description": "todo description",
  "author": "Alexander Kiriliuk",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint \"app/**/*.ts\" --fix",
    "init": "npm run init:config && npm run init:web-app",
    "init:config": "nest start init-config",
    "init:web-app": "nest start init-web-app --debug",
    "start:web-app": "nest start web-app",
    "start:web-app:dev": "nest start web-app --debug --watch",
    "start:web-app:prod": "node dist/app/web/main"
  },
  "dependencies": {
    "@k-platform/core": "0.0.30",
    "@nestjs/axios": "3.0.2",
    "@nestjs/common": "10.3.7",
    "@nestjs/core": "10.3.7",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/microservices": "10.3.7",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.7",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/schedule": "4.0.1",
    "adm-zip": "0.5.12",
    "axios": "1.6.8",
    "bcrypt": "5.1.1",
    "canvas": "2.11.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.6",
    "dotenv": "16.4.5",
    "exifreader": "4.21.1",
    "ffprobe": "1.1.2",
    "ffprobe-static": "3.1.0",
    "file-type": "19.0.0",
    "heic-convert": "2.1.0",
    "helmet": "7.1.0",
    "imagemin": "7.0.1",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-pngquant": "9.0.2",
    "ioredis": "5.3.2",
    "multer": "1.4.5-lts.1",
    "music-metadata": "7.14.0",
    "node-redis-warlock": "1.0.2",
    "object-hash": "3.0.0",
    "pg": "8.11.5",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "sharp": "0.33.3",
    "typeorm": "0.3.20",
    "uuid": "9.0.1",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.7",
    "@nestjs/typeorm": "10.0.2",
    "@types/adm-zip": "0.5.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "1.4.11",
    "@types/node": "20.12.2",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^@gen-src/(.*)$": "<rootDir>/app/gen-src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "coveragePathIgnorePatterns": [
      ".mock.ts",
      ".e2e-spec.ts",
      ".module.ts",
      ".entity.ts",
      ".subscriber.ts",
      "main.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/"
    ]
  }
}
